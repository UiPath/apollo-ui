{
  "name": "@uipath/ap-chat",
  "version": "1.2.3",
  "description": "Apollo Design System - Autopilot Chat web component",
  "repository": {
    "type": "git",
    "url": "https://github.com/UiPath/apollo-ui.git",
    "directory": "web-packages/ap-chat"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./service": {
      "types": "./dist/service.d.ts",
      "import": "./dist/service.js",
      "require": "./dist/service.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "NODE_ENV=production rsbuild build",
    "build:analyze": "NODE_ENV=production BUNDLE_ANALYZE=true rsbuild build",
    "dev": "NODE_ENV=development rsbuild dev",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --run --coverage",
    "clean": "rm -rf dist",
    "format": "biome format --write src",
    "format:check": "biome format src"
  },
  "keywords": [
    "design-system",
    "web-component",
    "chat",
    "autopilot",
    "apollo",
    "uipath"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^5.18.0",
    "@swc/helpers": "^0.5.17",
    "@uipath/apollo-react": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.7",
    "@rsbuild/plugin-react": "^1.1.2",
    "@rslib/core": "^0.17.0",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@vitest/coverage-v8": "^4.0.12",
    "@vitest/ui": "^4.0.12",
    "happy-dom": "^15.11.7",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5",
    "webpack-bundle-analyzer": "^5.0.1"
  }
}
