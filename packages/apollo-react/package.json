{
  "name": "@uipath/apollo-react",
  "version": "2.4.0",
  "description": "Apollo Design System - React component library with Material UI theming",
  "repository": {
    "type": "git",
    "url": "https://github.com/UiPath/apollo-ui.git",
    "directory": "packages/apollo-react"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./material": {
      "types": "./dist/material/index.d.ts",
      "import": "./dist/material/index.js",
      "require": "./dist/material/index.cjs"
    },
    "./material/theme": {
      "types": "./dist/material/theme/index.d.ts",
      "import": "./dist/material/theme/index.js",
      "require": "./dist/material/theme/index.cjs"
    },
    "./material/components": {
      "types": "./dist/material/components/index.d.ts",
      "import": "./dist/material/components/index.js",
      "require": "./dist/material/components/index.cjs"
    },
    "./material/components/*": {
      "types": "./dist/material/components/*.d.ts",
      "import": "./dist/material/components/*.js",
      "require": "./dist/material/components/*.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./core/tokens/*": "./dist/core/tokens/*",
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "import": "./dist/icons/index.js",
      "require": "./dist/icons/index.cjs"
    },
    "./icons/*": {
      "types": "./dist/icons/*.d.ts",
      "import": "./dist/icons/*.js",
      "require": "./dist/icons/*.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:icons && rslib build",
    "build:analyze": "ANALYZE=true pnpm run build",
    "build:icons": "tsx scripts/icons-build.ts",
    "dev": "rslib build --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist",
    "i18n:extract": "lingui extract",
    "i18n:extract:watch": "lingui extract --watch",
    "i18n:compile": "lingui compile"
  },
  "keywords": [
    "design-system",
    "react",
    "material-ui",
    "apollo",
    "uipath"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "react": ">=18.3.0",
    "react-dom": ">=18.3.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@lingui/core": "^5.6.1",
    "@lingui/react": "^5.6.1",
    "@mui/x-tree-view": "^8.21.0",
    "@uipath/apollo-core": "workspace:*",
    "@uipath/apollo-utils": "workspace:*",
    "date-fns": "^4.1.0",
    "debounce": "^3.0.0",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.27",
    "lodash": "^4.17.21",
    "lowlight": "^3.3.0",
    "react-dropzone": "^14.3.8",
    "react-focus-lock": "^2.13.7",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@lingui/cli": "^5.6.1",
    "@lingui/conf": "^5.6.1",
    "@lingui/format-json": "^5.6.1",
    "@lingui/macro": "^5.6.1",
    "@lingui/swc-plugin": "^5.9.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "5.15.3",
    "@mui/system": "5.15.15",
    "@mui/x-date-pickers": "^6.19.2",
    "@rsbuild/plugin-babel": "^1.0.6",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rslib/core": "^0.17.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.17.21",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "esbuild": "^0.27.0",
    "glob": "^13.0.0",
    "happy-dom": "^15.11.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14",
    "webpack-bundle-analyzer": "^5.0.1"
  }
}
