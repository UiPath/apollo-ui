{
  "name": "@uipath/apollo-utils",
  "version": "0.0.0",
  "description": "Apollo Design System - Shared utilities and helper functions",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest --run --passWithNoTests",
    "clean": "rm -rf dist",
    "release": "semantic-release -e semantic-release-monorepo"
  },
  "keywords": [
    "design-system",
    "utilities",
    "apollo",
    "uipath"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@rslib/core": "^0.17.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  }
}
