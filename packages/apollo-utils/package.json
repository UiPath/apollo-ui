{
  "name": "@uipath/apollo-utils",
  "version": "0.0.0",
  "description": "Apollo Design System - Shared utilities and helper functions",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
