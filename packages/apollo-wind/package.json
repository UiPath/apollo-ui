{
  "name": "@uipath/apollo-wind",
  "version": "0.0.0",
  "description": "Apollo Design System - Tailwind CSS and shadcn/ui implementation",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tailwind.config": "./tailwind.config.js"
  },
  "files": [
    "dist",
    "tailwind.config.js"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest --run --passWithNoTests",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "design-system",
    "tailwind",
    "shadcn",
    "react",
    "apollo",
    "uipath"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@uipath/apollo-core": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@rsbuild/plugin-react": "^1.4.1",
    "@rslib/core": "^0.17.0",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.14",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  }
}
