{
  "name": "@uipath/apollo-wind",
  "version": "0.10.0",
  "description": "UiPath wind design system - A Tailwind CSS based React component library",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css",
    "./tailwind.css": "./dist/tailwind.css",
    "./tailwind": {
      "import": "./dist/tailwind.preset.js",
      "require": "./dist/tailwind.preset.cjs"
    },
    "./postcss": {
      "import": "./dist/postcss.config.export.js",
      "require": "./dist/postcss.config.export.cjs"
    }
  },
  "files": [
    "dist",
    "postcss.config.export.js"
  ],
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build && npm run build:css",
    "build:css": "npx @tailwindcss/cli -i ./src/styles/tailwind.css -o ./dist/styles.css",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
