{
  "name": "@uipath/apollo-angular",
  "private": true,
  "version": "0.0.0",
  "description": "Apollo Design System - Angular component library with Angular Material theming",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "dev": "ng-packagr -p ng-package.json --watch",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "format": "biome format --write src",
    "format:check": "biome format src",
    "test": "echo 'No tests configured yet'",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "design-system",
    "angular",
    "angular-material",
    "apollo",
    "uipath"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "@angular/core": ">=19.0.0",
    "@angular/common": ">=19.0.0",
    "@angular/material": ">=19.0.0"
  },
  "dependencies": {
    "@uipath/apollo-core": "workspace:*",
    "@uipath/apollo-utils": "workspace:*"
  },
  "devDependencies": {
    "@angular/core": "^19.2.18",
    "@angular/common": "^19.2.18",
    "@angular/material": "^20.2.11",
    "@angular/compiler": "^19.2.18",
    "@angular/compiler-cli": "^19.2.18",
    "ng-packagr": "^20.3.0",
    "typescript": "~5.7.2"
  }
}
